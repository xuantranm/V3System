@model Ap.Business.ViewModels.DynamicProjectReportViewModel

<div>
    Kết quả: <b><span id="totalRecords">@Model.TotalRecords</span></b>
</div>
<div class="table-responsive">
    <table class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>Action</th>
                <th>PO Date</th>
                <th>POID</th>
                <th>PO Type</th>
                <th>Project Code</th>
                <th>Project Name</th>
                <th>Stock Code</th>
                <th>Stock Name</th>
                <th>Stock Type</th>
                <th>MRF</th>
                <th>Category</th>
                <th>Supplier</th>
                <th>Unit</th>
                <th>Quantity</th>
                <th>Received Quantity</th>
                <th>Pending Quantity</th>
                <th>Weight</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.DynamicProjectReports)
            {
                <tr id="item@(item.Id)">
                    <td>@item.Action</td>
                    <td>@item.PODate</td>
                    <td>@item.POCode</td>
                    <td>@item.POType</td>
                    <td>@item.ProjectCode</td>
                    <td>@item.ProjectName</td>
                    <td>@item.StockCode</td>
                    <td>@item.StockName</td>
                    <td>@item.StockType</td>
                    <td>@item.MRF</td>
                    <td>@item.Category</td>
                    <td>@item.Supplier</td>
                    <td>@item.Unit</td>
                    <td>@item.Quantity</td>
                    <td>@item.QuantityReceived</td>
                    <td>@item.QuantityPending</td>
                    <td>@item.Weight</td>
                </tr>
            }
            @if (!Model.DynamicProjectReports.Any())
            {
                <tr>
                    <td colspan="17" class="text-center">No data</td>
                </tr>
            }
        </tbody>
    </table>
</div>
@if (Model.TotalPages > 1)
{
    <div class="float-left">
        @Html.Partial("Partials/_PreparePagingPartial")
    </div>
}